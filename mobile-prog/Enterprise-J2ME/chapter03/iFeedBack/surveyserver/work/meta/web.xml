<!DOCTYPE web-app
 PUBLIC "-//Sun Microsystems, Inc.
          //DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>iFeedBackSurvey</display-name>
  <servlet>
    <servlet-name>Server</servlet-name>
    <servlet-class>
       com.enterprisej2me.iFeedBack.surveyserver.Server
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Server</servlet-name>
    <url-pattern>/servlet/Server</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>
